                <td>
                    <a href="{{ path('app_type_graphe_show', {'id': type_graphe.id}) }}">Details</a>
                    <a href="{{ path('app_type_graphe_edit', {'id': type_graphe.id}) }}">Modifier</a>

                </td>
            </tr>
        {% else %}
{#            <tr>#}
{#                <td colspan="12">no records found</td>#}
{#            </tr>#}
        {% endfor %}
        </tbody>
    </table>

    <a href="{{ path('app_type_graphe_new') }}">Nouveau type de graphe</a>
{% endblock %}
