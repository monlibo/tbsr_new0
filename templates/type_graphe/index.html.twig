{#                <th>CreatedBy</th>#}
{#                <th>UpdatedAt</th>#}
{#                <th>UpdatedBy</th>#}
{#                <th>DeletedAt</th>#}
{#                <th>DeletedBy</th>#}
                <th>Actions</th>
            </tr>
        </thead>
        <tbody>
        {% for type_graphe in type_graphes %}
            <tr>
{#                <td>{{ type_graphe.id }}</td>#}
                <td>{{ type_graphe.code }}</td>
                <td>{{ type_graphe.libelle }}</td>
{#                <td>{{ type_graphe.url }}</td>#}
{#                <td>{{ type_graphe.urlAbsolute }}</td>#}
{#                <td>{{ type_graphe.createdAt ? type_graphe.createdAt|date('Y-m-d H:i:s') : '' }}</td>#}
{#                <td>{{ type_graphe.createdBy }}</td>#}
